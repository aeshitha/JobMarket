<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxWidth="1920.0" prefHeight="900.0" prefWidth="900.0"
            style="-fx-background-color: green;" xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="frontEnd.ViewMessageController">
    <children>
        <Label fx:id="lblMain" alignment="CENTER" layoutX="-2.0" layoutY="20.0" prefHeight="30.0" prefWidth="900.0"
               style="-fx-background-color: royalblue;" textFill="#2f3640">
            <font>
                <Font name="System Bold" size="20.0"/>
            </font>
        </Label>
        <JFXListView fx:id="lView_Message" layoutX="10.0" layoutY="69.0" onKeyReleased="#lView_Message_onKeyReleased"
                     onMouseClicked="#lView_Message_onMouseClicked" prefHeight="748.0" prefWidth="880.0"/>
        <JFXButton fx:id="btn_cancel" layoutX="730.0" layoutY="836.0" onKeyReleased="#btn_cancel_onKeyReleased"
                   onMouseClicked="#btn_cancel_onMouseClicked" prefHeight="50.0" prefWidth="149.0"
                   style="-fx-background-color: white;" text="Cancel">
            <font>
                <Font size="20.0"/>
            </font>
        </JFXButton>
        <JFXButton fx:id="btn_complaint" layoutX="27.0" layoutY="836.0" onKeyReleased="#btn_complaint_onKeyReleased"
                   onMouseClicked="#btn_complaint_onMouseClicked" prefHeight="50.0" prefWidth="149.0"
                   style="-fx-background-color: white;" text="Complaint">
            <font>
                <Font size="20.0"/>
            </font>
        </JFXButton>
        <JFXButton fx:id="btn_allMe" layoutX="224.0" layoutY="836.0" onKeyReleased="#btn_allMe_onKeyReleased"
                   onMouseClicked="#btn_allMe_onMouseClicked" prefHeight="50.0" prefWidth="149.0"
                   style="-fx-background-color: white;" text="All">
            <font>
                <Font size="20.0"/>
            </font>
        </JFXButton>
        <JFXButton fx:id="btn_unreadMe" layoutX="379.0" layoutY="836.0" onKeyReleased="#btn_unreadMe_onKeyReleased"
                   onMouseClicked="#btn_unreadMe_onMouseClicked" prefHeight="50.0" prefWidth="149.0"
                   style="-fx-background-color: white;" text="Unread">
            <font>
                <Font size="20.0"/>
            </font>
        </JFXButton>
    </children>
</AnchorPane>
